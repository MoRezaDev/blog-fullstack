model User {
    id String @id @default(uuid())
    name String?
    mobile String @unique
    role UserRole @default(USER)
    image String?
    otp Otp?

    posts Post[]
    likes Like[]
    comments Comment[]

    favorite_posts FavoritePost?

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    

}

enum UserRole {
    ADMIN
    USER
}